<div id="container">
  <div id="user_header">
    <% if current_user%>
      Logged in as <%= current_user.name %>
      <%= link_to "Log Out", session_path("current"), method: "delete" %>
      <%= link_to "Admin Panel", admin_root_path %>
    <% else %>
      <%= link_to "Sign Up", new_user_path %>
      <%= link_to "Sign In", new_session_path %>
    <% end %>
  </div>
</div>
